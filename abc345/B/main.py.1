#!/usr/bin/env python3
import sys
import math

def solve(X: int):
    if X == 0:
        print(0)
        return
    if 0 < X < 10:
        print(1)
        return
    if -10 < X < 0:
        print(0)
        return
    p = int(str(X)[:-1])
    q = int(str(X)[-1])
    if X >= 0:
        if q == 0:
            print(p)
        else:
            print(p + 1)
    else:
        print(p)
    return


