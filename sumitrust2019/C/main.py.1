#!/usr/bin/env python3
import sys

def main():
    X = int(input())
    dp = [0] * (10 ** 5 + 1)
    for i in range(100, 106):
        dp[i] = 1
    
    for i in range(0, X + 1):
        for j in range(100, 106):
            if i - j < 0:
                continue
            dp[i] |= dp[i - j]
    
    print(dp[X])

if __name__ == '__main__':
    main()

