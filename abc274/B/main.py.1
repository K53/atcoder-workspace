#!/usr/bin/env python3




def main():
    H, W = map(int, input().split())
    G = []
    for _ in range(H):
        G.append(list(input()))

    res = []
    for i in range(W):
        ans = 0
        for j in range(H):
            if G[j][i] == "#":
                ans += 1
        res.append(ans)
    print(*res, sep=" ")



if __name__ == '__main__':
    main()
