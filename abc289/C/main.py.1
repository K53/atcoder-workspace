#!/usr/bin/env python3


def main():
    N, M = map(int, input().split())
    As = []
    for i in range(M):
        C = int(input())
        a = set(map(int, input().split()))
        As.append(a)

    ans = 0
    for i in range(1, 2 ** M):
        all = set()
        for b in range(M):
            if (i >> b) & 1:
                all |= As[b]
        for i in range(1, N + 1):
            if i not in all:
                break
        else:
            ans += 1
    print(ans)

if __name__ == '__main__':
    main()
